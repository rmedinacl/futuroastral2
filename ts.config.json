{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "allowJs": true,
      "checkJs": false,
      "sourceMap": true,
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }
  